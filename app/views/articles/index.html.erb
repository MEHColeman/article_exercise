<table>
  <tr>
    <th>Like</th>
    <th>ID</th>
    <th>Title</th>
    <th>Description</th>
    <th>Photo URL</th>
  </tr>

  <% @articles.each do |article| %>
    <tr>
      <td><%= render 'like_status', liked: article.liked?, article_id: article.id %></td>
      <td><%= article.id %></td>
      <td><%= article.title %></td>
      <td><%= article.description %></td>
      <td><%= article.photo_url %></td>
      <th>
      <% end %>
</table>
